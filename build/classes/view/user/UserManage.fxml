                        <Font name="System Bold" size="18.0" />
                     </font>
                  </JFXTextField>
                  <JFXComboBox fx:id="comboDepartmet" focusColor="RED" labelFloat="true" prefHeight="50.0" prefWidth="300.0" promptText="Department" unFocusColor="#0007ff" />
