                        <Font name="System Bold" size="18.0" />
                     </font>
                  </JFXTextField>
                  <JFXTextField fx:id="txtCPassword" focusColor="RED" labelFloat="true" prefHeight="50.0" prefWidth="300.0" promptText="Confirm Password " unFocusColor="#4000ff">
