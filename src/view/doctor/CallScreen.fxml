<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="768.0" prefWidth="1150.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.doctor.CallScreenController">
    <stylesheets>
        <URL value="@callscreen.css" />
    </stylesheets>
   <children>
      <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox alignment="TOP_CENTER" HBox.hgrow="ALWAYS">
               <children>
                  <Label alignment="CENTER" prefHeight="75.0" prefWidth="571.0" text="Doctor Screen">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                  </Label>
                  <TableView fx:id="table" prefHeight="768.0" prefWidth="681.0" VBox.vgrow="ALWAYS">
                    <columns>
                      <TableColumn fx:id="colNo" prefWidth="50.0" text="No" />
                      <TableColumn fx:id="colName" prefWidth="200.0" text="Patient Name" />
                        <TableColumn fx:id="colId" prefWidth="100.0" text="ID" />
                        <TableColumn fx:id="ColDoctor" prefWidth="120.0" text="Doctor Name" />
                        <TableColumn fx:id="colState" prefWidth="100.0" text="Status" />
                    </columns>
                  </TableView>
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" HBox.hgrow="ALWAYS">
               <children>
                  <Label alignment="CENTER" prefHeight="75.0" prefWidth="571.0" text="Cemo Check Screen">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                  </Label>
                  <TableView fx:id="cemoTable" layoutX="10.0" layoutY="10.0" prefHeight="768.0" prefWidth="681.0" VBox.vgrow="ALWAYS">
                     <columns>
                        <TableColumn fx:id="cemoNoCol" prefWidth="50.0" text="No" />
                        <TableColumn fx:id="cemoNameCol" prefWidth="200.0" text="Patient Name" />
                        <TableColumn fx:id="cemoIdCol" prefWidth="100.0" text="ID" />
                        <TableColumn fx:id="cemoStatusCol" prefWidth="222.0" text="Status" />
                     </columns>
                  </TableView>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
